{
    "name": "icicleio/postgres",
    "description": "Asynchronous PostgreSQL client for Icicle.",
    "keywords": [
        "database",
        "db",
        "postgresql",
        "postgre",
        "pgsql",
        "asynchronous",
        "async"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Icicle\\Postgres\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\Postgres\\": "tests"
        }
    }
}
